{
  "name": "effective-enigma",
  "repository": "https://github.com/samgj18/effective-enigma",
  "packageManager": "yarn@3.1.1",
  "private": true,
  "version": "0.0.1",
  "module": "./dist/index.mjs",
  "scripts": {
    "build": "rollup -c",
    "dev": "yarn eff:wrangler dev",
    "publish": "yarn eff:wrangler publish",
    "prepare": "husky install",
    "postinstall": "husky install && ts-patch install -s",
    "test": "jest --coverage",
    "wrangler": "wrangler",
    "eff:wrangler": "wrangler",
    "eff:format": "eslint --max-warnings=0 --fix && prettier --write '**/*.{js,ts,css,json,md}'",
    "eff:lint": "eslint --max-warnings=0 && prettier --check '**/*.{js,ts,css,json,md}'"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.0.0",
    "@cloudflare/wrangler": "^1.19.7",
    "@effect-ts/core": "^0.59.2",
    "@effect-ts/system": "^0.56.2",
    "@effect-ts/tracing-plugin": "^0.15.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/eslint": "^8.2.2",
    "@types/jest": "^26.0.23",
    "@types/service-worker-mock": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-typescript": "^3.0.0",
    "husky": "^7.0.4",
    "jest": "^27.0.1",
    "prettier": "^2.3.0",
    "rollup": "^2.36.1",
    "rollup-plugin-terser": "^7.0.2",
    "service-worker-mock": "^2.0.5",
    "ts-jest": "^27.0.1",
    "ts-patch": "^2.0.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@tsndr/cloudflare-worker-jwt": "^1.1.5"
  }
}
